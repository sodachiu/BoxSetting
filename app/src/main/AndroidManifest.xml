<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eileen.boxsetting">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NetSettingActivity"
            android:launchMode="singleTask" />
        <activity android:name=".WireSettingActivity" />
        <activity android:name=".BluetoothSettingActivity" />
        <activity android:name=".NetInfoActivity" />
        <activity android:name=".PPPoEActivity" />
        <activity android:name=".StaticIpActivity" />
        <activity android:name=".DateTimeActivity" />
        <activity android:name=".DateFormatActivity" />
        <activity android:name=".DisplayActivity" />
        <activity android:name=".ResolutionActivity" />
        <activity android:name=".OutputRegionActivity" />
        <activity android:name=".StoreInfoActivity" />
        <activity
            android:name=".DevDialogActivity"
            android:theme="@style/dialogStyle" />
        <activity android:name=".ResFactoryDialogActivity"
            android:theme="@style/dialogStyle"/>
        <activity android:name=".ResolutionDialogActivity" />
        <activity android:name=".AdvancedActivity" />
        <activity android:name=".ResFactoryActivity" />
        <activity android:name=".AdvancedItemActivity" />
        <activity android:name=".ClearAllActivity"></activity>
    </application>

</manifest>